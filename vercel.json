{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "npm run build"
      }
    }
  ],
  "build": {
    "env": {
      "NEXT_PUBLIC_VERCEL_URL": "${VERCEL_URL}"
    }
  }
}